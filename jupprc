mathins		^[ #
mathres		^[ =
msg		^[ H
msg		^[ h
nextw		^K N
nextw		^K ^N
nextw		^K n
play		^[ 0 TO 9
prevw		^K P
prevw		^K ^P
prevw		^K p
query		^[ ?
quote		`
quote8		^P
record		^[ (
retype		^Q L
retype		^Q ^L
retype		^Q l
retype		^[ q l
rtn		^M
shell		^K Z
shell		^K ^Z
shell		^K z
stop		^[ )

:Paste
type					^@ TO ÿ
rtn					^M
msg,"Entered bracketed paste mode",rtn	^[ [ 2 0 0 ~
helpcard,rtn,keymap,"main",rtn,msg,rtn	^[ [ 2 0 1 ~
helpcard,rtn,keymap,"main",rtn		^D

:Pastecua
type					^@ TO ÿ
rtn					^M
msg,"Entered bracketed paste mode",rtn	^[ [ 2 0 0 ~
helpcard,rtn,keymap,"cua",rtn,msg,rtn	^[ [ 2 0 1 ~
helpcard,rtn,keymap,"cua",rtn		^D

:Pasteprompt
type					^@ TO ÿ
nop					^L
keymap,"prompt",rtn,msg,rtn,rtn		^M
msg,"Entered bracketed paste mode",rtn	^[ [ 2 0 0 ~
keymap,"prompt",rtn,msg,rtn		^[ [ 2 0 1 ~
keymap,"prompt",rtn			^D

:main
:inherit windows
